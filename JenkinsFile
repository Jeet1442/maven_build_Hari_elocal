pipeline{
   agent any
   tools{
     maven 'M3'
   }
   stages{
     stage('Checkout'){
       steps{
	     git branch: 'main', url: 'https://github.com/Jeet1442/maven_build_Hari_elocal.git'
	   }
    }
	stage('Build'){
	  steps{
	    bat 'mvn clean'
	  }
	}
	stage('Install'){
	  steps{
	    bat 'mvn install'
		
	  }
	}
	stage('TEST'){
	  steps{
	    bat 'mvn test'
		junit '**/target/surefire-reports/TEST-*.xml'
	  }
	}
  }
}