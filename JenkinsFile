pipeline{
   agent any
   tools{
     maven 'M3'
   }
   stages{
     stage('Checkout, Test, Publish'){
       steps{
	     git branch: 'main', url: 'https://github.com/Jeet1442/maven_build_Hari_elocal.git'
		 script{
		    bat(/mvn clean test/)
		 }
		 
		 step([$class : 'Publisher', reportFilenamePattern : '**/target/surefire-reports/TEST-*.xml'])
	   }
    }
	
	
	
  }
}